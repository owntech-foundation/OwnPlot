  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-win": "electron-forge make --platform squirrel",
    "make-mac": "electron-forge make --platform darwin",
    "pack-clean": "rm -rf dist",
    "pack-linux": "electron-builder --linux --x64",
    "pack-win": "electron-builder --win --x64",
    "pack-mac-uni": "electron-builder --mac --universal",
    "pack-linux-all": "electron-builder --linux --x64 --armv7l --arm64",
    "pack-win-all": "electron-builder --win --ia32 --x64 --arm64",
    "pack-all": "pack-clean; pack-mac-uni; pack-linux-all; pack-win-all"
  },
  "build": {
    "appId": "com.ownplot.app",
    "win": {
      "target": "nsis",
      "icon": "assets/Icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/Icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/Icon.png",
      "category": "Network",
      "synopsis": "Serial plotter",
      "description": "Serial plotter"
    }
  },
  "author": "OwnTech",
  "contributors": [
    "Guillaume Arthaud <garthaud@laas.fr>",
    "Matthias Riffard <mriffard@laas.fr>"
  ],
  "bugs": "https://gitlab.laas.fr/owntech/ownplot/-/issues",
  "license": "LGPL-2.1",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/Icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "OwnPlot"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
